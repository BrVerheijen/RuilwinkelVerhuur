name: Build and deploy ASP.Net Core app to Azure Web App - RuilwinkelVerhuur

on:
  push:
    branches:
      - ProductionBranch
  workflow_dispatch:

jobs:
  build:
    runs-on: windows-latest

    steps:
    - name: ZAP Scan
    uses: zaproxy/action-baseline@v0.4.0
    with:
      target: 'https://www.zaproxy.org'
      
